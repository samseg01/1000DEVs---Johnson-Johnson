import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	
	Scanner teclado = new Scanner(System.in);
	
	int saMi;
	int saFu;
	int salaFi;
	
	
	System.out.print("Informe o valor do sálario mínimo: ");
	sami = teclado.nextInt();
	
	System.out.print("Qual o salário nominal do funcionario: ");
	safu = teclado.nextInt();
	System.out.print("\n");
	
	salafi = (safu / sami);
	System.out.print("O funcionario recebe " + salafi + " salarios minimos");
	
	
	}
}


